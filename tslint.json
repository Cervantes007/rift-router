{
  "defaultSeverity": "warning",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "variable-name": false,
    "function-name": false,
    "no-boolean-literal-compare": false,
    "strict-boolean-expressions": false,
    "no-increment-decrement": false,
    "import-name": false
  }
}
